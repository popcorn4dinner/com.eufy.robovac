{
  "$extends": [
		"rf.ir_remote"
	],
  "id": "eufy-robovac",
  "name": {
    "en": "eufy Robovac"
  },
  "class": "vacuumcleaner",
  "images": {
    "large": "/drivers/eufy-robovac/assets/images/large.png",
    "small": "/drivers/eufy-robovac/assets/images/small.png"
  },
  "capabilities": [
		"start_auto_cleaning",
		"return_to_charging_base",
		"start_cleaning",
		"pause_cleaning",
		"start_single_room_cleaning",
    "start_spot_cleaning",
    "start_edge_cleaning",
    "start_max_cleaning"
	],
	"rf": {
		"signal": "lg_tv",
		"minTxInterval": 900,
		"cmdNumberPrefix": "DIGIT_",
		"capabilityToCommandMap": {
			"start_auto_cleaning": "AUTO",
		  "return_to_charging_base": "HOME",
		  "start_cleaning": "START",
		  "pause_cleaning": "PAUSE",
		  "start_single_room_cleaning": "ROOM",
      "start_spot_cleaning": "SPOT",
      "start_edge_cleaning": "EDGE",
      "start_max_cleaning": "MAX"
		}
	},
	"$flow": {
		"actions": [
			{
				"id": "{{driverId}}:send_cmd",
				"title": {
					"en": "Send Command"
				},
				"args": [
					{
						"name": "cmd",
						"type": "autocomplete"
					}
				]
			}
		]
	}
}
